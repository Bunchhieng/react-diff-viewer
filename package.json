{
  "name": "@bunchhieng/react-diff-viewer",
  "version": "3.1.2",
  "private": false,
  "description": "A simple and beautiful text diff viewer component made with diff and React",
  "keywords": [
    "review",
    "code-review",
    "diff",
    "diff-viewer",
    "github",
    "react",
    "react-component",
    "ui"
  ],
  "repository": "git@github.com:Bunchhieng/react-diff-viewer.git",
  "license": "MIT",
  "author": "Pranesh Ravi<praneshpranesh@gmail.com>",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc --outDir lib/",
    "lint": "eslint . --ext .js",
    "build:examples": "webpack",
    "build:watch": "tsc --outDir lib/ -w",
    "publish:examples": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist -r $GITHUB_REPO_URL",
    "publish:examples:local": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist",
    "start:examples": "webpack serve --config webpack.config.js",
    "test": "mocha --require ts-node/register --require enzyme.ts ./test/**",
    "test:watch": "mocha --require ts-node/register --require enzyme.ts --watch-extensions ts,tsx --watch ./test/**"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "clsx": "^1.2.1",
    "create-emotion": "^11.0.0",
    "diff": "^5.1.0",
    "enzyme": "^3.11.0",
    "memoize-one": "^6.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "@types/expect": "^1.20.3",
    "@types/memoize-one": "^5.1.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.0.12",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^4.4.13",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "css-loader": "^6.7.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "expect": "^24.8.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^6.1.4",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.0.2",
    "spy": "^1.0.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
